class Book:
    def __init__(self, title, author, year_published):
        self.title = title
        self.author = author
        self.year_published = year_published

    def describe(self):
        print(f"Title: {self.title}, Author: {self.author}, Year Published: {self.year_published}")

book1 = Book("Teenage Mutan Ninja Turtle", "Kevin Eastman and Peter Laird", 1984)
book2 = Book("Spider Man", " Stan Lee and artist Steve Ditko", 1962)
book3 = Book("Fast and Furious", "Gary Scott Thompson, Erik Bergquist, and David Ayer", 2001)

book1.describe()
book2.describe()
book3.describe()
